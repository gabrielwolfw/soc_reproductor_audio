module top (
    input        CLOCK_50,
    input  [3:0] KEY,
    output [27:0] HEX_DISPLAY,
    // Audio codec connections
    input        AUD_BCLK,
    output       AUD_DACDAT,
    input        AUD_DACLRCK,
    input        RESET_N
);

    // Instancia del sistema generado por Qsys
    socaudio u_socaudio (
        .audio_BCLK(AUD_BCLK),
        .audio_DACDAT(AUD_DACDAT),
        .audio_DACLRCK(AUD_DACLRCK),
        .buttons_export(KEY),
        .clk_clk(CLOCK_50),
        .reset_reset_n(RESET_N),
        .seven_segments_export(HEX_DISPLAY)
    );

endmodule